{
  "name": "cong-app",
  "productName": "Cong Electron Application Employee Management System",
  "description": "Cong Electron Application Employee Management System",
  "version": "2.0.0",
  "license": "MIT",
  "author": {
    "name": "Ubakong",
    "email": "por@ubakong.com",
    "url": "http://www.ubakong.com/"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/arnuns/cong-app.git"
  },
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:electron": "ng build --base-href ./ && electron .",
    "electron-build": "ng build --base-href --prod",
    "package-mac": "electron-packager . 'Cong App' --overwrite --asar=true --platform=darwin --arch=x64 --icon=src/assets/icons/cong_icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . 'Cong App' --overwrite --asar --platform=win32 --arch=ia32 --icon=src/assets/icons/cong_icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Cong App\"",
    "create-installer-mac": "electron-installer-dmg './release-builds/Cong App-darwin-x64/Cong App.app' 'Cong App' --out=release-builds --overwrite --icon=src/assets/icons/cong_icon.icns",
    "create-installer-win": "node installers/windows/createinstaller.js"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.803.14",
    "@angular/animations": "~8.2.12",
    "@angular/common": "~8.2.12",
    "@angular/compiler": "~8.2.12",
    "@angular/core": "~8.2.12",
    "@angular/forms": "~8.2.12",
    "@angular/platform-browser": "~8.2.12",
    "@angular/platform-browser-dynamic": "~8.2.12",
    "@angular/router": "~8.2.12",
    "core-js": "^3.3.4",
    "electron-updater": "^4.1.2",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular/cli": "~8.3.14",
    "@angular/compiler-cli": "~8.2.12",
    "@angular/language-service": "~8.2.12",
    "@types/jasmine": "~3.4.4",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.11.7",
    "codelyzer": "~5.2.0",
    "electron": "^7.0.0",
    "electron-builder": "^21.2.0",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^14.0.6",
    "electron-winstaller": "^4.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.0",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0",
    "typescript": "^3.4.5"
  }
}
